<section class="page-title style3" data-path="img/headers/header10.jpeg">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <h1 class="strong text-uppercase">What we Enjoy</h1>
        <ol class="breadcrumb hidden-xs">
          <li><a href="/"><i class="fa fa-home"></i></a></li>
          <li><a href="/articles">Blog</a></li>
          <li class="active">Masonry 4cols</li>
        </ol>
      </div>
    </div>
  </div>
</section>

<section class="featured-posts">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">

            <h1 class="strong">Keep up with us</h1>
            <div class="cbp cbp-slider-edge featured-slider">
              <% @articles.all.order('created_at DESC').each do |blog_post| %>
              <div class="cbp-item">
                <a href="articles/<%= blog_post.id %>" title="Read full post">
                  <div class="featured-post" data-path="<%= blog_post.imagelink %>" style="background: url(img/blog/large/1.jpg);">
                    <div class="post-content">
                      <p class="label label-warning post-category"></p>
                      <h3 class="post-title"><%= blog_post.title%></h3>
                    </div>
                    <div class="post-meta">
                      <span class="post-date"><%= blog_post.created_at %></span>

                    </div>
                  </div>
                </a>
              </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </section>





              <%#= blog_post.description %>






